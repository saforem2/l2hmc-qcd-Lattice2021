@article{Kaya:2018lar,
	abstract = {Real Space Renormalization Group (RSRG) treatment of Ising model for square and simple cubic lattice is investigated and critical coupling strengths of these lattices are obtained. The mathematical complications, which appear inevitable in the decimated partition function due to Block-spin transformation, is treated with a relevant approximation from outset. The approximation is based on the approximate equivalence of \$\textbackslash ln(1+f(K,\textbackslash\{\textbackslash sigma\_\{n.n\}\textbackslash\})) \textbackslash simeq f(K,\textbackslash\{\textbackslash sigma\_\{n.n\}\textbackslash\})\$ for small \$f(K,\textbackslash\{\textbackslash sigma\_\{n.n\}\textbackslash\})\$, here \$K\$ is the nearest neighbor coupling strength and \$\textbackslash\{\textbackslash sigma\_\{n.n\}\textbackslash\}\$ is the nearest neighbor spins degrees of freedom around a central spin. The values of the critical coupling strengths are obtained as \$0.4830\$ for square lattice and \$0.2225\$ for simple cubic (SC) lattice. The corresponding critical exponents values \$\textbackslash alpha\$ and \$\textbackslash nu\$ are also calculated within very acceptable agreement with those values obtained from numerical works},
	archiveprefix = {arXiv},
	author = {Kaya, Tuncer},
	date = {2018-01-16},
	eprint = {1801.05385},
	eprinttype = {arxiv},
	keywords = {\#nosource,Condensed Matter - Statistical Mechanics},
	primaryclass = {cond-mat},
	title = {A {{Proper Approximate Real Space Renormalization Group Treatment}} of {{Ising}} Model {{For Square}} and {{Simple Cubic Lattice}}},
	url = {http://arxiv.org/abs/1801.05385},
	urldate = {2018-01-27},
