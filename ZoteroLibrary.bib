	issn = {0031-9007, 1079-7114},
	journaltitle = {Physical Review Letters},
	keywords = {Computer Science - Machine Learning,Condensed Matter - Statistical Mechanics,Statistics - Machine Learning},
	number = {26},
	pages = {260601},
