	number = {5},
	pages = {054002},
	shortjournal = {J. Phys. Soc. Jpn.},
	title = {Logical {{Reasoning}} for {{Revealing}} the {{Critical Temperature}} through {{Deep Learning}} of {{Configuration Ensemble}} of {{Statistical Systems}}},
