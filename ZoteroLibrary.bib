@article{cardyUnusualCorrectionsScaling2010,
	abstract = {We present a general theory of the corrections to the asymptotic behaviour of the R??nyi entropies S A ( n ) = (1 ??? n ) ??? 1 logTr?? A n which measure the entanglement of an interval A of length \#\#IMG\#\# [http://ej.iop.org/images/1742-5468/2010/04/P04023/jstat352740ieqn1.gif] \textbackslash ell with the rest of an infinite one-dimensional system, in the case when this is described by a conformal field theory of central charge c . These can be due to bulk irrelevant operators of scaling dimension x {$>$} 2, in which case the leading corrections are of the expected form \#\#IMG\#\# [http://ej.iop.org/images/1742-5468/2010/04/P04023/jstat352740ieqn2.gif] \textbackslash ell\^-2(x-2) for values of n close to 1. However, for n {\$>\$} x /( x ??? 2) corrections of the form \#\#IMG\#\# [http://ej.iop.org/images/1742-5468/2010/04/P04023/jstat352740ieqn3.gif] \textbackslash ell\^2-x-x/n and \#\#IMG\#\# [http://ej.iop.org/images/1742-5468/2010/04/P04023/jstat352740ieqn4.gif] \textbackslash ell\^-2x/n arise and dominate the conventional terms. We also point out that the last type of corrections can also occur with x less than 2. They arise from relevant operators induced by the conical spacetime singularities necessary to describe the reduced density matrix. These agree with recent analytic and numerical results for quantum spin chains. We also compute the effect of marginally irrelevant bulk operators, which give a correction \#\#IMG\#\# [http://ej.iop.org/images/1742-5468/2010/04/P04023/jstat352740ieqn5.gif] \O{}r ((\textbackslash log \textbackslash ell)\^-2) , with a universal amplitude. We present analogous results for the case when the interval lies at the end of a semi-infinite system.},
	author = {Cardy, John and Calabrese, Pasquale},
	date = {2010},
	journaltitle = {Journal of Statistical Mechanics: Theory and Experiment},
	keywords = {\#nosource},
	number = {04},
	pages = {P04023},
	title = {Unusual Corrections to Scaling in Entanglement Entropy},
	url = {http://stacks.iop.org/1742-5468/2010/i=04/a=P04023},
	volume = {2010},
	bdsk-url-1 = {http://stacks.iop.org/1742-5468/2010/i=04/a=P04023}}

